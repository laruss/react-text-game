{
    "$schema": "https://typedoc.org/schema.json",
    "entryPointStrategy": "packages",
    "entryPoints": [
        "packages/core",
        "packages/ui"
    ],
    "packageOptions": {
        "entryPoints": [
            "src/index.ts"
        ]
    },
    "out": "api-docs",
    "name": "React Text Game",
    "includeVersion": true,
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": false,
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": false
    },
    "categorizeByGroup": true,
    "navigationLinks": {
        "Documentation": "https://your-docs-url.com"
    },
    "readme": "README.md",
    "plugin": ["typedoc-plugin-markdown"],
    "treatWarningsAsErrors": false,
    "externalSymbolLinkMappings": {
        "@types/react": {
            "React.Component.render": "#",
            "React.StaticLifecycle.getDerivedStateFromProps": "#",
            "React.ComponentLifecycle.componentDidMount": "#"
        },
        "@react-text-game/core": {
            "SimpleObject": "#",
            "SimpleObjectImpl": "#"
        }
    }
}
